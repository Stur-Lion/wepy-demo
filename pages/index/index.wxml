<!--index.wxml-->
<view class="app-container">
  <swiper indicator-dots="{{indicatorDots}}" circular="true" class="swiper-box">
    <block wx:for="{{imgUrls}}" wx:key="{{index}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" mode="widthFix"/>
      </swiper-item>
    </block>
  </swiper>
</view>
